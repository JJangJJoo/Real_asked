    <h1>Ask anything!</h1>
    <form action = "/ask">
      이름 :
      <select name = "name">
          <option value="anonym">익명</option>
          <option value="<%= session[:email] %>">내 아이디</option>
      </select><br>
      질문 :
      <input type = "text", name = "question"><br>
      <input type ="submit">
    </form>
    <% @questions.each do |q| %>
      <p>질문자 : <%= q.name %></p>
      <p>내용 :<%= q.content %></p>
    <% end %>
